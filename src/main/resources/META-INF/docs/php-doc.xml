<idea-plugin>
    <extensions defaultExtensionNs="cn.yiiguxing.plugin.translate">
        <documentation.elementFilter
                language="PHP"
                implementationClass="cn.yiiguxing.plugin.translate.documentation.filter.PhpDocumentationElementFilter"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij">
        <lang.documentationProvider
                id="Translation.TranslatedDocumentationProvider.PHP"
                order="first"
                language="PHP"
                implementationClass="cn.yiiguxing.plugin.translate.documentation.provider.TranslatedDocumentationProvider"/>
    </extensions>
</idea-plugin>